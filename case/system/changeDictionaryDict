FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}

boundary
{
    "wedge.*"
    {
        type            wedge;
        physicalType    wedge;
    }
    aircraft
    {
        type            wall;
        physicalType    wall;
    }
    tunnel
    {
        type            slip;
        physicalType    slip;
    }
}
